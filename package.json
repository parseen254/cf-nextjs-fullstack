{
  "name": "cf-nextjs-fullstack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run pages:build --watch & wrangler pages dev --local --d1=fullstack-nextjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev",
    "deploy": "npm run pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "generate": "npx drizzle-kit generate",
    "migrate": "npx wrangler d1 migrations apply fullstack-nextjs --local",
    "migrate-prod": "npx wrangler d1 migrations apply fullstack-nextjs --remote"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.7.3",
    "@aws-sdk/client-ses": "^3.686.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.36.0",
    "lucide-react": "^0.454.0",
    "next": "^14.2.16",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "drizzle-kit": "^0.26.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-next-on-pages": "^1.13.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^37.12.1",
    "wrangler": "^3.84.1"
  }
}
